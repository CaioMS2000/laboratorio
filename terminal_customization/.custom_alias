alias fullupdate="sudo apt update && clear && sudo apt list --upgradable && sleep 5 && sudo apt upgrade -y"
alias Cvirtualenv="python3 -m venv $PY_VIRTUAL_ENV_FOLDER"
alias virtualenv="source $PY_VIRTUAL_ENV_FOLDER/bin/activate"
alias laboratorio="cd /home/$USER/dev/laboratorio && git pull"
alias socialnetwork="cd /home/$USER/dev/social\ network && git pull"
alias sunlight="cd /home/$USER/dev/sun-light && git pull"
#alias reset_docker="docker rmi $(docker images -a -q)"
alias reset_docker="docker rmi $(echo $(docker images -a -q))"
alias rush_commit="git add . && git commit --allow-empty-message -m \"\""
alias rush_migrations="python manage.py makemigrations && python manage.py migrate"